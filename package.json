{
  "name": "crib",
  "version": "0.1.0",
  "description": "Web UI for crib",
  "main": "index.ts",
  "scripts": {
    "dev": "parcel serve src/index.html -d dist --public-url /dist/ --open",
    "build": "cross-env NODE_ENV=production parcel build src/index.html -d build --public-url .",
    "clean": "rimraf .cache/ && rimraf dist/ && rimraf build/",
    "test:unit": "jest --no-cache",
    "test:coverage": "jest --no-cache --coverage",
    "predeploy": "npm run build"
  },
  "author": "David Zuber",
  "license": "GPL-3.0-or-later",
  "devDependencies": {
    "autoprefixer": "^8.0.0",
    "babel-core": "^6.26.0",
    "babel-jest": "^22.4.1",
    "babel-plugin-module-resolver": "^3.1.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "cross-env": "^5.1.3",
    "jest": "^22.4.2",
    "jest-serializer-vue": "^0.3.0",
    "node-sass": "^4.7.2",
    "parcel-bundler": "^1.6.2",
    "parcel-plugin-vue": "^1.5.0",
    "rimraf": "^2.6.2",
    "tslint": "^5.9.1",
    "tslint-config-standard": "^7.0.0",
    "typescript": "^2.7.2",
    "vue-jest": "^1.4.0",
    "vue-template-compiler": "^2.5.13",
    "vue-test-utils": "^1.0.0-beta.11"
  },
  "dependencies": {
    "vue": "^2.5.13",
    "vue-class-component": "^6.2.0",
    "vue-property-decorator": "^6.0.0",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1",
    "vuex-class": "^0.3.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "jest": {
    "moduleFileExtensions": [
      "js",
      "vue"
    ],
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      ".*\\.(vue)$": "<rootDir>/node_modules/vue-jest"
    },
    "mapCoverage": true,
    "coverageDirectory": "./coverage/",
    "collectCoverage": true
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:storax/crib-web-ui.git"
  },
  "bugs": {
    "url": "https://github.com/storax/crib-web-ui/issues"
  },
  "homepage": "https://github.com/storax/crib-web-ui#readme"
}
